<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mapper.DishCommentsMapper">

    <delete id="deleteDishCommentsByRstuserIds">
        delete from dishcomments where rstuserId in
        <foreach collection="rstuserIds"  item="rstuserId" open="(" close=")" separator=",">
            #{rstuserId}
        </foreach>
    </delete>
</mapper>